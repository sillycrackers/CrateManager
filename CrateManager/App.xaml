             xmlns:views="clr-namespace:CrateManager.Views"
             xmlns:viewModels="clr-namespace:CrateManager.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    
    <Application.Resources>


        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Styles1.xaml"/>

                <ResourceDictionary>
                    <FontFamily x:Key="VarelaRound"> pack://application:,,,/Fonts/#Varela Round</FontFamily>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>


    </Application.Resources>
</Application>
